{
    "$schema": "http://json.schemastore.org/template",
    "author": "Viv",
    "classifications": [
        "Class"
    ],
    "identity": "VivClassTemplate",
    "name": "Custom Class with Namespace",
    "shortName": "vivclass",
    "sourceName": "Demo",
    "symbols": {
        "namespace": {
            "type": "parameter",
            "description": "Namespace for the class",
            "defaultValue": "VivNamespace",
            "replaces": "Demos.Demo"
        },
        "classname": {
            "type": "parameter",
            "description": "Name of the class",
            "defaultValue": "VivClass",
            "replaces": "VivClass"
        }
    }
}
